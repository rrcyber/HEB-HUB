<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:ns0="www.tibco.com/plugin/Sleep" xmlns:ns02="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx="http://xmlns.heb.com/ei/PURCHASEORDER_INFORMATION_COMPLETED" xmlns:pfx2="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://xmlns.heb.com/ei/PURCHASEORDER_INFORMATION_COMPLETED" schemaLocation="/Shared Resources/Schema Definitions/Custom/PURCHASEORDER_INFORMATION_COMPLETED_SOAP.xsd"/>
    <pd:name>Project Name Processes/Starter Process/Product_Reprocess_Starter.process</pd:name>
    <pd:startName>Timer</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="Timer">
        <pd:type>com.tibco.plugin.timer.TimerEventSource</pd:type>
        <pd:resourceType>ae.activities.timer</pd:resourceType>
        <pd:x>132</pd:x>
        <pd:y>240</pd:y>
        <config>
            <StartTime>1380303000000</StartTime>
            <Frequency>false</Frequency>
            <TimeInterval>%%TIMER_CONF/INTERVAL_REPROCESS%%</TimeInterval>
            <FrequencyIndex>Millisecond</FrequencyIndex>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>1002</pd:endX>
    <pd:endY>242</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1371748043362</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>331</pd:x>
        <pd:y>130</pd:y>
        <pd:width>464</pd:width>
        <pd:height>209</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:outSlot>reprocessList</pd:outSlot>
            <pd:activityOutputName>Map Data</pd:activityOutputName>
            <pd:repeatCondition>count($Get-JMS-Queue-Message/pfx2:ActivityOutput)=0</pd:repeatCondition>
            <pd:accumulateOutput>true</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Get JMS Queue Message</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Confirm</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Null</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>One Bend</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Get JMS Queue Message</pd:from>
            <pd:to>Map Data</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Get JMS Queue Message</pd:from>
            <pd:to>Null</pd:to>
            <pd:xpathDescription>Empty Queue</pd:xpathDescription>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Map Data</pd:from>
            <pd:to>Confirm</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Confirm">
            <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
            <pd:resourceType>activity.confirm</pd:resourceType>
            <pd:x>675</pd:x>
            <pd:y>197</pd:y>
            <config>
                <ConfirmEvent>Get JMS Queue Message</ConfirmEvent>
            </config>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="Get JMS Queue Message">
            <pd:type>com.tibco.plugin.jms.JMSQueueGetMessageActivity</pd:type>
            <pd:resourceType>ae.activities.JMSQueueGetMessageActivity</pd:resourceType>
            <pd:x>400</pd:x>
            <pd:y>204</pd:y>
            <config>
                <PermittedMessageType>XML Text</PermittedMessageType>
                <SessionAttributes>
                    <transacted>false</transacted>
                    <acknowledgeMode>2</acknowledgeMode>
                    <maxSessions>1</maxSessions>
                    <destination>%%Destinations/Queues/PMM_PRODUCT_REPROCESS_QUEUE%%</destination>
                    <selector>JMSCorrelationID='PRODUCT'</selector>
                </SessionAttributes>
                <ConfigurableHeaders>
                    <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                    <JMSExpiration>0</JMSExpiration>
                    <JMSPriority>4</JMSPriority>
                </ConfigurableHeaders>
                <ConnectionReference>/Shared Resources/Connections/JMS/JMS Connection Mex.sharedjmscon</ConnectionReference>
                <OutDataxsdString>
                    <xsd:element name="group">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="ITEM" minOccurs="0" maxOccurs="unbounded">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="audit_number" type="xsd:string"/>
                                            <xsd:element name="prd_lvl_number" type="xsd:string"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </OutDataxsdString>
            </config>
            <pd:inputBindings>
                <pfx2:ActivityInput>
                    <timeout>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Connections/JMS/Properties/GLB_JMS_REPROCESS_TIMEOUT"/>
                    </timeout>
                </pfx2:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Null">
            <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
            <pd:resourceType>ae.activities.null</pd:resourceType>
            <pd:x>557</pd:x>
            <pd:y>271</pd:y>
            <config/>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="Map Data">
            <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
            <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
            <pd:x>558</pd:x>
            <pd:y>197</pd:y>
            <config>
                <element>
                    <xsd:element name="group">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="ITEM" minOccurs="0" maxOccurs="unbounded">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="audit_number" type="xsd:string"/>
                                            <xsd:element name="prd_lvl_number" type="xsd:string"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </element>
            </config>
            <pd:inputBindings>
                <group>
                    <xsl:for-each select="$Get-JMS-Queue-Message/pfx2:ActivityOutput/Body/group/ITEM">
                        <ITEM>
                            <audit_number>
                                <xsl:value-of select="audit_number"/>
                            </audit_number>
                            <prd_lvl_number>
                                <xsl:value-of select="prd_lvl_number"/>
                            </prd_lvl_number>
                        </ITEM>
                    </xsl:for-each>
                </group>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Product_Reprocess_update">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>881</pd:x>
        <pd:y>152</pd:y>
        <config>
            <processName>/Project Name Processes/Sub Process/Product_Reprocess_update.process</processName>
        </config>
        <pd:inputBindings>
            <group>
                <xsl:for-each select="$reprocessList/AccumulatedOutput/group/ITEM">
                    <ITEM>
                        <AUDIT_NUMBER>
                            <xsl:value-of select="audit_number"/>
                        </AUDIT_NUMBER>
                        <prd_lvl_number>
                            <xsl:value-of select="tib:trim(prd_lvl_number)"/>
                        </prd_lvl_number>
                    </ITEM>
                </xsl:for-each>
            </group>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Product_Reprocess_update</pd:to>
        <pd:xpathDescription>COUNT&gt;0</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>count($reprocessList/AccumulatedOutput/group)&gt;0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Product_Reprocess_update</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Timer</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>