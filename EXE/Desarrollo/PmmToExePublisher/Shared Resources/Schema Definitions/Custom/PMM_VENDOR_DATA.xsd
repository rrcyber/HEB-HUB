						<xsd:element name="BAS_ZIP" type="xsd:string" nillable="true"/>
						<xsd:element name="CNTRY_CODE" type="xsd:string" nillable="true"/>
						<xsd:element name="BAS_CONTACT_NAME" type="xsd:string" nillable="true"/>
						<xsd:element name="VEND_PHONE" type="xsd:string" nillable="true"/>
						<xsd:element name="VPC_EMAIL" type="xsd:string" nillable="true"/>
						<xsd:element name="PRYMARY_EMAIL" type="xsd:string" nillable="true"/>
						<xsd:element name="TRAN_TYPE" type="xsd:string" nillable="true"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>