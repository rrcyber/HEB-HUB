<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>PmmItemSync Processes/Sub Process/Update_SDIVPCCSI.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="BATCH_NUM" type="xsd:string"/>
    </pd:startType>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>450</pd:endX>
    <pd:endY>64</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1450718669260</pd:targetNamespace>
    <pd:activity name="Update_SC_SDIVPCCSI">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>286</pd:x>
        <pd:y>65</pd:y>
        <config>
            <timeout>%%Connections/DataBase/BDHEBUS/GLB_TIMEOUT%%</timeout>
            <commit>false</commit>
            <emptyStrAsNil>true</emptyStrAsNil>
            <jdbcSharedConfig>/Shared Resources/Connections/Database/JDBC_BDHEBUS.sharedjdbc</jdbcSharedConfig>
            <statement>update SC_SDIVPCCSI set FLAG = '2'
WHERE BATCH_NUM = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>BATCH_NUM</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <BATCH_NUM>
                    <xsl:value-of select="$Start/BATCH_NUM"/>
                </BATCH_NUM>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Update_SC_SDIVPCCSI</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Update_SC_SDIVPCCSI</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>