                </prd_lvl_number>
                <prd_lvl_number1>
                    <xsl:copy-of select="$Start/pfx3:FIXUPCData/pfx3:PRD_LVL_NUMBER/@xsi:nil"/>
                    <xsl:value-of select="$Start/pfx3:FIXUPCData/pfx3:PRD_LVL_NUMBER"/>
                </prd_lvl_number1>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
