                                        <xsl:value-of select="pfx2:Code"/>
                                    </pfx2:Code>
                                    <pfx2:Reason>
                                        <xsl:value-of select="pfx2:Reason"/>
                                    </pfx2:Reason>
                                    <pfx2:Detail>
                                        <xsl:value-of select="pfx2:Detail"/>
                                    </pfx2:Detail>
                                </pfx2:Fault>
                            </xsl:for-each>
                        </pfx2:Body>
                    </xsl:for-each>
                </pfx2:MSG_CONTAINER>
            </xsl:for-each>
