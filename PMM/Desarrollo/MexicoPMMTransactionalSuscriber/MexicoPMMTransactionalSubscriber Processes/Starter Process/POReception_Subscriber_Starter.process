<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:pfx2="http://xmlns.heb.com/ei/ErrorManagementSchema" xmlns:pfx3="http://xmlns.heb.com/ei/INNER_PACK_INFORMATION" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx4="http://xmlns.heb.com/ei/PRODUCT_CODE_INFORMATION" xmlns:ns0="www.tibco.com/plugin/Sleep" xmlns:ns02="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx5="http://xmlns.heb.com/ei/PURCHASEORDER_INFORMATION" xmlns:pfx6="http://xmlns.heb.com/ei/PORECEPTION_INFORMATION">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://xmlns.heb.com/ei/PURCHASEORDER_INFORMATION" schemaLocation="/Shared Resources/Schema Definitions/Custom/PURCHASEORDER_INFORMATION_SOAP.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://xmlns.heb.com/ei/PORECEPTION_INFORMATION" schemaLocation="/Shared Resources/Schema Definitions/Custom/PORECEPTION_INFORMATION_SOAP.xsd"/>
    <pd:name>MexicoPMMTransactionalSubscriber Processes/Starter Process/POReception_Subscriber_Starter.process</pd:name>
    <pd:startName>JMS Queue Receiver</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="JMS Queue Receiver">
        <pd:type>com.tibco.plugin.jms.JMSQueueEventSource</pd:type>
        <pd:resourceType>ae.activities.JMSQueueEventSource</pd:resourceType>
        <pd:x>164</pd:x>
        <pd:y>154</pd:y>
        <config>
            <PermittedMessageType>XML Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>2</acknowledgeMode>
                <maxSessions>%%Connections/JMS/EMS/MIG_EMS_MAX_SESSIONS%%</maxSessions>
                <destination>%%Destinations/Queues/PORECEPTION_STG_QUEUE%%</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/Shared Resources/Connections/JMS/JMS Connection.sharedjmscon</ConnectionReference>
            <OutDataxsdString ref="pfx6:MSG_CONTAINER"/>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>784</pd:endX>
    <pd:endY>154</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1273678251231</pd:targetNamespace>
    <pd:group name="Retry Group on Error">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>255</pd:x>
        <pd:y>25</pd:y>
        <pd:width>330</pd:width>
        <pd:height>242</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>count</pd:indexSlot>
            <pd:errorCondition>$count &gt;= $_globalVariables/pfx:GlobalVariables/RetryConfig/GBL_MAX_RETRIES</pd:errorCondition>
            <pd:suspendAfterErrorRetry>true</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>POReception_Insert_Main</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>POReception_Insert_Main</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>POReception_Insert_Main</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Straight</pd:lineType>
            <pd:lineColor>-3407872</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Retry</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="POReception_Insert_Main">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>364</pd:x>
            <pd:y>80</pd:y>
            <config>
                <processName>/MexicoPMMTransactionalSubscriber Processes/Main Process/Purchase Order Reception/POReception_Insert_Main.process</processName>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$JMS-Queue-Receiver/ns1:ActivityOutput/Body/pfx6:MSG_CONTAINER/pfx6:Body/pfx6:PURCHASE_ORDER_RECEPTION"/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>366</pd:x>
            <pd:y>213</pd:y>
            <config/>
            <pd:inputBindings>
                <ns0:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/RetryConfig/GBL_RETRY_DELAY"/>
                    </IntervalInMillisec>
                </ns0:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Retry">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>550</pd:x>
            <pd:y>213</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns02:ActivityInput>
                    <message>
                        <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                    </message>
                    <xsl:if test="$_error/ns:ErrorReport/MsgCode">
                        <messageCode>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </messageCode>
                    </xsl:if>
                </ns02:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Confirm">
        <pd:type>com.tibco.pe.core.ConfirmActivity</pd:type>
        <pd:resourceType>activity.confirm</pd:resourceType>
        <pd:x>690</pd:x>
        <pd:y>154</pd:y>
        <config>
            <ConfirmEvent>JMS Queue Receiver</ConfirmEvent>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="ErrorProcess">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>690</pd:x>
        <pd:y>277</pd:y>
        <config>
            <processName>/Utility Services/Exception Handling/ErrorProcess.process</processName>
        </config>
        <pd:inputBindings>
            <pfx2:MSG_Container>
                <pfx2:Header>
                    <pfx2:ProjectName>
                        <xsl:value-of select="$_processContext/ns:ProcessContext/ProjectName"/>
                    </pfx2:ProjectName>
                    <pfx2:Source_Domain>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/ProjectConfig/Source_Domain"/>
                    </pfx2:Source_Domain>
                    <pfx2:Destination_Domain>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/ProjectConfig/Destination_Domain"/>
                    </pfx2:Destination_Domain>
                    <pfx2:Transaction_Type>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/ProjectConfig/Transaction_Type"/>
                    </pfx2:Transaction_Type>
                    <pfx2:TimeStamp>
                        <xsl:value-of select="tib:format-dateTime(&quot;yyyy-MM-dd hh:mm:ss&quot;, current-dateTime())"/>
                    </pfx2:TimeStamp>
                </pfx2:Header>
                <pfx2:Body>
                    <pfx2:ErrorManagementSchema>
                        <pfx2:inrtn_id>
                            <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/ExceptionHandling/MIG_ERR_INTEGRATION_ID"/>
                        </pfx2:inrtn_id>
                        <pfx2:inrtn_instn_nm>
                            <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/ExceptionHandling/GLB_ERR_INSTANCE_NAME"/>
                        </pfx2:inrtn_instn_nm>
                        <pfx2:inrtn_err_rtn_desc>
                            <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                        </pfx2:inrtn_err_rtn_desc>
                        <pfx2:inrtn_msg_body>
                            <xsl:value-of select="tib:render-xml($JMS-Queue-Receiver/ns1:ActivityOutput/Body/pfx6:MSG_CONTAINER)"/>
                        </pfx2:inrtn_msg_body>
                        <pfx2:cre8_ts>
                            <xsl:value-of select="tib:format-dateTime(&quot;yyyy-MM-dd hh:mm:ss&quot;, current-dateTime())"/>
                        </pfx2:cre8_ts>
                        <xsl:if test="$_error/ns:ErrorReport/MsgCode">
                            <pfx2:u_code>
                                <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                            </pfx2:u_code>
                        </xsl:if>
                        <pfx2:sev_id>
                            <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/ExceptionHandling/GLB_ERR_SEV_MED"/>
                        </pfx2:sev_id>
                    </pfx2:ErrorManagementSchema>
                </pfx2:Body>
            </pfx2:MSG_Container>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>JMS Queue Receiver</pd:from>
        <pd:to>Retry Group on Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Retry Group on Error</pd:from>
        <pd:to>Confirm</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Confirm</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Retry Group on Error</pd:from>
        <pd:to>ErrorProcess</pd:to>
        <pd:lineType>Straight</pd:lineType>
        <pd:lineColor>-3407872</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ErrorProcess</pd:from>
        <pd:to>Confirm</pd:to>
        <pd:lineType>Straight</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>