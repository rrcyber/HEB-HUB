JOIN mag_import_flex_promo_hub h ON (p.source_promo_id = h.source_promo_id)
join RelacionSucursalWebSite rw on (h.website = rw.WebsiteId and rw.TipoRelacion = 'PromoLogix')
WHERE control_date IS NULL
AND complete_promo IS NOT NULL
AND p.complete_promo &lt;= ?</statement>
