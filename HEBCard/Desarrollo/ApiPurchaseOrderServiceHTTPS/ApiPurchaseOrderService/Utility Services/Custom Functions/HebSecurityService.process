        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <URI>
                    <xsl:value-of select="concat($_globalVariables/ns1:GlobalVariables/Connections/HTTPS/HEBSecurity/CFG_URL, &quot;/api/application/&quot;, $Start/HebSecurity/Application, &quot;/login/&quot;, $Start/HebSecurity/User, &quot;/object/&quot;, $Start/HebSecurity/Object, &quot;/validate&quot;)"/>
