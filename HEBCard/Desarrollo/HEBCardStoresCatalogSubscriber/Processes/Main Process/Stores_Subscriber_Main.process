<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx6="http://xmlns.heb.com/ei/CatalogoPlannerList" xmlns:pfx7="http://xmlns.heb.com/ei/PriceInventory" xmlns:pfx8="http://xmlns.heb.com/ei/Bitacora" xmlns:pfx="http://xmlns.heb.com/ei/PO_NOTIFY" xmlns:pfx9="http://xmlns.heb.com/ei/Products" xmlns:pfx2="http://www.tibco.com/schemas/PmmPoPendingPublisher/Shared Resources/Schema Definitions/Common/Schema.xsd3" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:pfx3="http://www.tibco.com/ns/no_namespace_schema_location/Shared Resources/Schema Definitions/Common/Exception.xsd" xmlns:pfx4="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx5="http://xmlns.heb.com/ei/CatalogoPlanner" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:ns2="www.tibco.com/plugin/Sleep" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns4="http://xmlns.heb.com/ei/Price" xmlns:ns3="http://xmlns.heb.com/ei/PlannerDepartmentList" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns5="http://xmlns.heb.com/ei/Inventory" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:ns12="http://xmlns.heb.com/ei/error" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx11="http://www.tibco.com/ns/no_namespace_schema_location/Shared Resources/Schema Definitions/Custom/ALTA_SOCIO_PEOPLE_SOFT_API.xsd" xmlns:pfx12="http://xmlns.heb.com/ei/PMM_CUSTOMER_DATA" xmlns:pfx10="http://xmlns.heb.com/ei/Socio">
    <xsd:import namespace="http://www.tibco.com/schemas/PmmPoPendingPublisher/Shared Resources/Schema Definitions/Common/Schema.xsd3"/>
    <xsd:import namespace="http://xmlns.heb.com/ei/PO_NOTIFY" schemaLocation="/Shared Resources/Schema Definitions/Common/PO_NOTIFY_SOAP.xsd"/>
    <xsd:import namespace="http://xmlns.heb.com/ei/PO_NOTIFY" schemaLocation="/Shared Resources/Schema Definitions/Custom/PO_NOTIFY_SOAP.xsd"/>
    <xsd:import namespace="http://www.tibco.com/ns/no_namespace_schema_location/Shared Resources/Schema Definitions/Common/Exception.xsd" schemaLocation="/Shared Resources/Schema Definitions/Common/Exception.xsd"/>
    <xsd:import namespace="http://xmlns.heb.com/ei/CatalogoPlanner" schemaLocation="/Shared Resources/Schema Definitions/Custom/CATALOGO_PLANNER_INFORMATION_DATA_SOAP.xsd"/>
    <xsd:import namespace="http://xmlns.heb.com/ei/Products" schemaLocation="/Shared Resources/Schema Definitions/Custom/MAG_IMPORT_PRODUCTS_SOAP.xsd"/>
    <xsd:import namespace="http://xmlns.heb.com/ei/Socio" schemaLocation="/Shared Resources/Schema Definitions/Custom/ALTA_SOCIO_PEOPLE_SOFT_SOAP.xsd"/>
    <xsd:import namespace="http://www.tibco.com/ns/no_namespace_schema_location/Shared Resources/Schema Definitions/Custom/ALTA_SOCIO_PEOPLE_SOFT_API.xsd" schemaLocation="/Shared Resources/Schema Definitions/Custom/ALTA_SOCIO_PEOPLE_SOFT_API.xsd"/>
    <xsd:import namespace="http://xmlns.heb.com/ei/Socio" schemaLocation="/Shared Resources/Schema Definitions/Custom/PMM_CUSTOMER_DATA.xsd"/>
    <xsd:import namespace="http://xmlns.heb.com/ei/PMM_CUSTOMER_DATA" schemaLocation="/Shared Resources/Schema Definitions/Custom/PMM_CUSTOMER_DATA_SOAP.xsd"/>
    <pd:name>Processes/Main Process/Stores_Subscriber_Main.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Store" type="xsd:string"/>
    </pd:startType>
    <pd:startX>169</pd:startX>
    <pd:startY>224</pd:startY>
    <pd:returnBindings>
        <Output>
            <xsl:value-of select="if (string-length($Invoke_AddStore/Output)!=0)then&#xA;$Invoke_AddStore/Output&#xA;else&#xA;$Invoke_UpdStore/Output"/>
        </Output>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="Output" type="xsd:string"/>
    </pd:endType>
    <pd:endX>705</pd:endX>
    <pd:endY>213</pd:endY>
    <pd:errorSchemas>
        <Exception ref="pfx3:Exception"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1406154803290</pd:targetNamespace>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>453</pd:x>
        <pd:y>362</pd:y>
        <config>
            <faultName>Exception</faultName>
        </config>
        <pd:inputBindings>
            <pfx4:ActivityInput>
                <xsl:if test="$_error/ns:ErrorReport/Msg">
                    <message>
                        <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                    </message>
                </xsl:if>
                <xsl:if test="$_error/ns:ErrorReport/MsgCode">
                    <messageCode>
                        <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                    </messageCode>
                </xsl:if>
                <data>
                    <Exception>
                        <Domain>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Domain"/>
                        </Domain>
                        <Deployment>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Deployment"/>
                        </Deployment>
                        <Project>
                            <xsl:value-of select="$_processContext/ns:ProcessContext/ProjectName"/>
                        </Project>
                        <EngineName>
                            <xsl:value-of select="$_processContext/ns:ProcessContext/EngineName"/>
                        </EngineName>
                        <TimeStamp>
                            <xsl:value-of select="current-dateTime()"/>
                        </TimeStamp>
                        <Code>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </Code>
                        <Message>
                            <xsl:value-of select="concat( 'ERROR MESSAGE AT STORE :  ' ,$Parse-XML/pfx12:MSG_CONTAINER/pfx12:Body/pfx12:PMM_CUSTOMER[1]/pfx12:ITEM[1]/pfx12:CUSTOMERNUMBER ,' ' , tib:render-xml($_error/ns:ErrorReport ,0, 0))"/>
                        </Message>
                    </Exception>
                </data>
            </pfx4:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>330</pd:x>
        <pd:y>362</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Invoke_AddStore">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>593</pd:x>
        <pd:y>144</pd:y>
        <config>
            <processName>/Processes/Sub Process/Invoke_AddStore.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Get_Store_Inf/StoreRequest"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get_Store_Inf">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>367</pd:x>
        <pd:y>225</pd:y>
        <config>
            <processName>/Processes/Sub Process/Get_Store_Inf.process</processName>
        </config>
        <pd:inputBindings>
            <group>
                <Store>
                    <xsl:value-of select="$Parse-XML/pfx12:MSG_CONTAINER/pfx12:Body/pfx12:PMM_CUSTOMER[1]/pfx12:ITEM[1]/pfx12:CUSTOMERNUMBER"/>
                </Store>
                <AuditNumber>
                    <xsl:value-of select="$Parse-XML/pfx12:MSG_CONTAINER/pfx12:Body/pfx12:PMM_CUSTOMER[1]/pfx12:ITEM[1]/pfx12:AUDIT_NUMBER"/>
                </AuditNumber>
            </group>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Parse XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>268</pd:x>
        <pd:y>223</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>false</validateOutput>
            <term ref="pfx12:MSG_CONTAINER"/>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$Start/Store"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Operation">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>461</pd:x>
        <pd:y>224</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Invoke_UpdStore">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>592</pd:x>
        <pd:y>301</pd:y>
        <config>
            <processName>/Processes/Sub Process/Invoke_UpdateStore.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Get_Store_Inf/StoreRequest"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Invoke_AddStore</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Parse XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Invoke_UpdStore</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Operation</pd:from>
        <pd:to>Invoke_UpdStore</pd:to>
        <pd:xpathDescription>UPD</pd:xpathDescription>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>Get_Store_Inf</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Get_Store_Inf</pd:from>
        <pd:to>Operation</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Operation</pd:from>
        <pd:to>Invoke_AddStore</pd:to>
        <pd:xpathDescription>INS</pd:xpathDescription>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Parse-XML/pfx12:MSG_CONTAINER/pfx12:Body/pfx12:PMM_CUSTOMER/pfx12:ITEM/pfx12:ACTIONCODE !='M'</pd:xpath>
    </pd:transition>
</pd:ProcessDefinition>