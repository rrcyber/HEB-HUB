        <pd:inputBindings>
            <Request>
                <Header>
                    <TransactionID>
                        <xsl:value-of select="$XML-Start/ns3:ActivityOutputClass/Request/Header/TransactionID"/>
                    </TransactionID>
                    <Origin>
                        <xsl:value-of select="$XML-Start/ns3:ActivityOutputClass/Request/Header/Origin"/>
                    </Origin>
                </Header>
                <Customer>
                    <xsl:if test="$Start/MainRequest/DynamicParameters/Parameter[Name = &quot;Customer&quot;]/Value">
                        <ID>
                            <xsl:value-of select="$Start/MainRequest/DynamicParameters/Parameter[Name = &quot;Customer&quot;]/Value"/>
                        </ID>
                    </xsl:if>
                    <xsl:if test="$XML-Start/ns3:ActivityOutputClass/Request/Customer/SetID">
                        <SetID>
                            <xsl:value-of select="$XML-Start/ns3:ActivityOutputClass/Request/Customer/SetID"/>
                        </SetID>
                    </xsl:if>
                    <xsl:for-each select="$XML-Start/ns3:ActivityOutputClass/Request/Customer/Contacts">
                        <Contacts>
                            <xsl:if test="ID">
                                <ID>
                                    <xsl:value-of select="ID"/>
                                </ID>
                            </xsl:if>
                            <xsl:if test="Type">
                                <Type>
                                    <xsl:value-of select="Type"/>
                                </Type>
                            </xsl:if>
                            <xsl:if test="Name">
                                <Name>
                                    <xsl:value-of select="Name"/>
                                </Name>
                            </xsl:if>
                            <xsl:if test="Email">
                                <Email>
                                    <xsl:value-of select="Email"/>
                                </Email>
                            </xsl:if>
                            <xsl:if test="PrefCtMethod">
                                <PrefCtMethod>
                                    <xsl:value-of select="PrefCtMethod"/>
                                </PrefCtMethod>
                            </xsl:if>
                            <xsl:if test="AddressIdRef">
                                <AddressIdRef>
                                    <xsl:value-of select="AddressIdRef"/>
                                </AddressIdRef>
                            </xsl:if>
                            <xsl:if test="EffectiveDate">
                                <EffectiveDate>
                                    <xsl:value-of select="EffectiveDate"/>
                                </EffectiveDate>
                            </xsl:if>
                            <xsl:for-each select="Phones">
                                <Phones>
                                    <xsl:if test="IsPrimary">
                                        <IsPrimary>
                                            <xsl:value-of select="IsPrimary"/>
                                        </IsPrimary>
                                    </xsl:if>
                                    <xsl:if test="Type">
                                        <Type>
                                            <xsl:value-of select="Type"/>
                                        </Type>
                                    </xsl:if>
                                    <xsl:if test="CountryCode">
                                        <CountryCode>
                                            <xsl:value-of select="CountryCode"/>
                                        </CountryCode>
                                    </xsl:if>
                                    <xsl:if test="Number">
                                        <Number>
                                            <xsl:value-of select="Number"/>
                                        </Number>
                                    </xsl:if>
                                    <xsl:if test="Extension">
                                        <Extension>
                                            <xsl:value-of select="Extension"/>
                                        </Extension>
                                    </xsl:if>
                                </Phones>
                            </xsl:for-each>
                        </Contacts>
                    </xsl:for-each>
                </Customer>
            </Request>
        </pd:inputBindings>
    </pd:activity>
