                            </xsl:if>
                        </xsl:if>
                        <xsl:if test="current()/IsPrimary = &quot;Y&quot;">
                            <xsl:if test="IsPrimary">
                                <pfx7:PRIMARY_SHIP_TO>
                                    <xsl:value-of select="IsPrimary"/>
                                </pfx7:PRIMARY_SHIP_TO>
                            </xsl:if>
                        </xsl:if>
                        <xsl:if test="current()/IsPrimary = &quot;Y&quot;">
