                            </xsl:if>
                            <xsl:if test="County">
                                <pfx7:ADDRESS4>
                                    <xsl:value-of select="County"/>
                                </pfx7:ADDRESS4>
                            </xsl:if>
                            <xsl:if test="City">
                                <pfx7:CITY>
