                            </xsl:if>
                            <xsl:if test="County">
                                <pfx7:COUNTY>
                                    <xsl:value-of select="County"/>
                                </pfx7:COUNTY>
                            </xsl:if>
                            <xsl:if test="State">
                                <pfx7:STATE>
                                    <xsl:value-of select="State"/>
