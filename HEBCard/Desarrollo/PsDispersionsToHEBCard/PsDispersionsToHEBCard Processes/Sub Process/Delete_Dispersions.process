<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <pd:name>PsDispersionsToHebCard Processes/Sub Process/Delete_Dispersions.process</pd:name>
    <pd:description>Servicio que elimina los registros enviados</pd:description>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Account">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Dispersions" minOccurs="0" maxOccurs="unbounded">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="EmployeeId" type="xsd:string"/>
                                <xsd:element name="Benefit" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>126</pd:startX>
    <pd:startY>155</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>426</pd:endX>
    <pd:endY>155</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1539123046478</pd:targetNamespace>
    <pd:activity name="JDBC Update">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>276</pd:x>
        <pd:y>155</pd:y>
        <config>
            <timeout>%%Connections/Database/PSHR/GLB_TIMEOUT%%</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <statement>DELETE
FROM
	PS_HEB_BONOST_TMP B
WHERE
	B.EMPLID     = ?
	AND B.PIN_NM = ?</statement>
            <jdbcSharedConfig>/Shared Resources/Connections/Database/JDBC_PSHR.sharedjdbc</jdbcSharedConfig>
            <batchUpdate>true</batchUpdate>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>EMPLID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>PIN_NM</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <xsl:for-each select="$Start/Account/Dispersions">
                    <Record>
                        <EMPLID>
                            <xsl:value-of select="EmployeeId"/>
                        </EMPLID>
                        <PIN_NM>
                            <xsl:value-of select="Benefit"/>
                        </PIN_NM>
                    </Record>
                </xsl:for-each>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>JDBC Update</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Update</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>