            <pd:to>Error ProcessBD</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$i = $_globalVariables/pfx3:GlobalVariables/Heb2bArticuloPropuestaPublisherVariables/GLB_MAX_RETRIES</pd:xpath>
        </pd:transition>
        <pd:activity name="getDataDescuento">
            <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
            <pd:x>279</pd:x>
            <pd:y>156</pd:y>
            <config>
                <timeout>%%Connections/Database/DBHEB2B/GLB_TIMEOUT%%</timeout>
                <commit>false</commit>
                <maxRows>%%Connections/Database/DBHEB2B/GLB_MAX_ROWS%%</maxRows>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>SELECT DISTINCT PAD.PORCENTAJE, PAD.FECHAINICIO, PAD.FECHAFINAL, RTRIM(P.IDPMM)PROVEEDOR, TPD.IDPMM, TPD.DESCRIPCION
FROM PROPUESTAARTICULO PA 
INNER JOIN PROPUESTAARTICULOSDESCUENTO PAD ON PA.ID = PAD.IDPROPUESTAARTICULO
LEFT JOIN TIPODESCUENTO TPD ON PAD.IDTIPODESCUENTO = TPD.ID 
INNER JOIN PROVEEDOR P ON PA.IDPROVEEDOR = P.ID
WHERE PA.ID = ?</statement>
                <jdbcSharedConfig>/Shared Resources/Connections/Database/JDBC_HEBB.sharedjdbc</jdbcSharedConfig>
                <wizardData/>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>IdPropuestaArticulo</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
                <oraObjects/>
                <oraTables/>
                <QueryOutputCachedSchemaColumns>PORCENTAJE</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>6</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>float</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>FECHAINICIO</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>93</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>datetime</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>FECHAFINAL</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>93</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>datetime</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>PROVEEDOR</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>nvarchar</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>IDPMM</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>varchar</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>DESCRIPCION</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>varchar</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            </config>
            <pd:inputBindings>
                <jdbcQueryActivityInput>
                    <IdPropuestaArticulo>
                        <xsl:choose>
                            <xsl:when test="exists($Start/Items/Item)">
                                <xsl:value-of select="$Start/Items/Item"/>
                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </IdPropuestaArticulo>
                </jdbcQueryActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>282</pd:x>
            <pd:y>375</pd:y>
            <config/>
            <pd:inputBindings>
                <ns0:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/pfx3:GlobalVariables/Heb2bArticuloPropuestaPublisherVariables/GLB_RETRY_DELAY"/>
                    </IntervalInMillisec>
                </ns0:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="JDBC Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>393</pd:x>
            <pd:y>375</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns02:ActivityInput>
                    <message>
                        <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                    </message>
                    <xsl:if test="$_error/ns:ErrorReport/MsgCode">
