                    </Proveedor>
                    <TipoDescuentoPMM>
                        <xsl:value-of select="IDPMM"/>
                    </TipoDescuentoPMM>
                    <Descripcion>
                        <xsl:value-of select="DESCRIPCION"/>
                    </Descripcion>
                </Descuentos>
            </xsl:for-each>
        </SDIVPCAWI>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx8:SDIVPCAWI"/>
    <pd:endX>528</pd:endX>
    <pd:endY>273</pd:endY>
