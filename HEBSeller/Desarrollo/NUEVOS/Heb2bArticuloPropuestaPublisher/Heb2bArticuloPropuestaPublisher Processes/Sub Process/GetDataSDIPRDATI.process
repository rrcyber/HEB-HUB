        </config>
        <pd:inputBindings>
            <SDIPRDATI>
                <Temporada>
                    <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/TEMPORADA"/>
                </Temporada>
                <TipoMarca>
                    <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/TIPOMARCA"/>
                </TipoMarca>
                <CicloVida>
                    <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/CICLOVIDA"/>
                </CicloVida>
                <xsl:choose>
                    <xsl:when test="string-length($getDataArticulo/resultSet/Record[1]/MARCA) > 0">
                        <Marca>
                            <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/MARCA"/>
                        </Marca>
                    </xsl:when>
                    <xsl:otherwise>
                        <Marca>
                            <xsl:value-of select="'Error'"/>
                        </Marca>
                    </xsl:otherwise>
                </xsl:choose>
                <PaisOrigen>
                    <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/PAISORIGEN"/>
                </PaisOrigen>
                <xsl:choose>
                    <xsl:when test="$getDataArticulo/resultSet/Record[1]/ESIMPORTADO ='1' or  $getDataArticulo/resultSet/Record[1]/ESIMPORTADO ='true'">
                        <Importado>
                            <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/ESIMPORTADO"/>
                        </Importado>
                    </xsl:when>
                    <xsl:otherwise>
                        <Importado>
                            <xsl:value-of select="0"/>
                        </Importado>
                    </xsl:otherwise>
                </xsl:choose>
                <TipoDistribucion>
                    <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/TIPODISTRIBUCION"/>
                </TipoDistribucion>
                <TipoOrden>
                    <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/TIPOORDEN"/>
                </TipoOrden>
                <FlujoCat>
                    <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/FLUJOCAT"/>
                </FlujoCat>
                <xsl:choose>
                    <xsl:when test="$getDataArticulo/resultSet/Record[1]/ESBREAKPACK ='1' or $getDataArticulo/resultSet/Record[1]/ESBREAKPACK ='true'">
                        <BreakPack>
                            <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/ESBREAKPACK"/>
                        </BreakPack>
                    </xsl:when>
                    <xsl:otherwise>
                        <BreakPack>
                            <xsl:value-of select="0"/>
                        </BreakPack>
                    </xsl:otherwise>
                </xsl:choose>
                <xsl:choose>
                    <xsl:when test="string-length($getDataArticulo/resultSet/Record[1]/TIPOSKU) > 0">
                        <TipoSku>
                            <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/TIPOSKU"/>
                        </TipoSku>
                    </xsl:when>
                    <xsl:otherwise>
                        <TipoSku>
                            <xsl:value-of select="'Error'"/>
                        </TipoSku>
                    </xsl:otherwise>
                </xsl:choose>
                <Composicion>
                    <xsl:value-of select="$getDataArticulo/resultSet/Record[1]/COMPOSICION"/>
                </Composicion>
            </SDIPRDATI>
        </pd:inputBindings>
