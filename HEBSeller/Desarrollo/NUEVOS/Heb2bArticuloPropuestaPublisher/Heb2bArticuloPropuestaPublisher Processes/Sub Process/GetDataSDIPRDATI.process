        <pd:activity name="getDataArticulo">
            <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
            <pd:x>290</pd:x>
            <pd:y>152</pd:y>
            <config>
                <timeout>%%Connections/Database/DBHEB2B/GLB_TIMEOUT%%</timeout>
                <commit>false</commit>
                <maxRows>%%Connections/Database/DBHEB2B/GLB_MAX_ROWS%%</maxRows>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>SELECT  RTRIM(TEMP.IdPMM) TEMPORADA, RTRIM(CV.IDPMM) CICLOVIDA, RTRIM(M.IDPMM) MARCA, RTRIM(PO.IDPMM) PAISORIGEN, PA.ESIMPORTADO, 
                RTRIM(TD.IDPMM) TIPODISTRIBUCION, RTRIM(TOR.IDPMM) TIPOORDEN, RTRIM(FC.IDPMM) FLUJOCAT, PAC.ESBREAKPACK, 
                RTRIM(TS.IDPMM) TIPOSKU, RTRIM(C.IDPMM) COMPOSICION, RTRIM(TM.IDPMM) TIPOMARCA 
FROM PROPUESTAARTICULO PA 
LEFT JOIN PROPUESTAARTICULOESTILOATRIBUTO PAE ON PA.ID = PAE.IDPROPUESTAARTICULO 
LEFT JOIN PROPUESTAARTICULOCASEPACK PAC ON PA.ID = PAC.IDPROPUESTAARTICULO 
LEFT JOIN CICLOVIDA CV ON PA.IDCICLOVIDA = CV.ID 
LEFT JOIN MARCA M ON PA.IDMARCA = M.ID 
LEFT JOIN PAISORIGEN PO ON PA.IDPAISORIGEN = PO.ID 
LEFT JOIN TIPODISTRIBUCION TD ON PA.IDTIPODISTRIBUCION = TD.ID 
LEFT JOIN TIPOORDEN TOR ON PA.IDTIPOORDEN = TOR.ID 
LEFT JOIN FLUJOCAT FC ON PA.IDFLUJOCAT = FC.ID 
LEFT JOIN TIPOSKU TS ON PA.IDTIPOSKU = TS.ID 
LEFT JOIN COMPOSICION C ON PAE.IDCOMPOSICION = C.ID 
LEFT JOIN TIPOMARCA TM ON M.IDTIPOMARCA = TM.ID
LEFT JOIN Temporada TEMP on PA.IdTemporada = TEMP.ID
WHERE PA.ID = ?</statement>
                <jdbcSharedConfig>/Shared Resources/Connections/Database/JDBC_HEBB.sharedjdbc</jdbcSharedConfig>
                <Prepared_Param_DataType>
                    <parameter>
