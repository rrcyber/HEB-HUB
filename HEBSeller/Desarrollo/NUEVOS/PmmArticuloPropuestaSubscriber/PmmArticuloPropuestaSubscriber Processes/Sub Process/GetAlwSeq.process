    </pd:startType>
    <pd:startX>27</pd:startX>
    <pd:startY>269</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="empty($GetAlwSeq/resultSet/Record[1]/VPC_ALW_SEQ)=true()">
                <vpc_alw_seq>
                    <xsl:value-of select="1"/>
                </vpc_alw_seq>
            </xsl:when>
            <xsl:otherwise>
                <vpc_alw_seq>
                    <xsl:value-of select="$GetAlwSeq/resultSet/Record[1]/VPC_ALW_SEQ"/>
                </vpc_alw_seq>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="vpc_alw_seq" type="xsd:string"/>
    </pd:endType>
    <pd:endX>550</pd:endX>
    <pd:endY>270</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1389130102045</pd:targetNamespace>
    <pd:group name="Retry on error">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>113</pd:x>
        <pd:y>99</pd:y>
        <pd:width>330</pd:width>
        <pd:height>328</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:errorCondition>$i &gt;= $_globalVariables/pfx3:GlobalVariables/PmmArticuloPropuestaSubscriberVariables/GLB_MAX_RETRIES</pd:errorCondition>
            <pd:suspendAfterErrorRetry>true</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>GetAlwSeq</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>GetAlwSeq</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>GetAlwSeq</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-52429</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>JDBC Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Error ProcessBD</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$i = $_globalVariables/pfx3:GlobalVariables/PmmArticuloPropuestaSubscriberVariables/GLB_MAX_RETRIES</pd:xpath>
        </pd:transition>
        <pd:activity name="GetAlwSeq">
            <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
            <pd:x>286</pd:x>
            <pd:y>159</pd:y>
            <config>
                <timeout>%%Connections/Database/DBPMM/GLB_TIMEOUT%%</timeout>
                <commit>false</commit>
                <maxRows>%%Connections/Database/DBPMM/GLB_MAX_ROWS%%</maxRows>
                <emptyStrAsNil>true</emptyStrAsNil>
                <statement>select max(vpc.VPC_ALW_SEQ) vpc_alw_seq
from pmm.vpcalwee vpc, pmm.vpcmstee mst
 where  vpc.vpc_tech_key = mst.vpc_tech_key and mst.vendor_number= ?</statement>
                <jdbcSharedConfig>/Shared Resources/Connections/Database/JDBC_PMM.sharedjdbc</jdbcSharedConfig>
                <wizardData>#Sat Feb 08 19:01:48 CST 2014&#xD;
useSchemaInFQN=true&#xD;
columncount=0&#xD;
manualSQLText=SELECT UM.IDPMM UNIDADVENTA, PAP.PRECIOVENTASUGERIDO,\n\t\tCASE WHEN PAP.IDFORMATO IS NOT NULL THEN SF.IDPMM\n\t\t\tWHEN PAP.IDCLUSTER IS NOT NULL THEN SC.IDPMM\n\t\t\tELSE S.IDPMM\n\t\tEND AS IDSUCURSAL,\n\t\t(SELECT F.IDPMM FROM FORMATO F\n\t\t\tINNER JOIN SUCURSAL S ON F.ID \= S.IDFORMATO WHERE S.IDPMM \= IDSUCURSAL) IDFORMATO\nFROM PROPUESTAARTICULO PA\nINNER JOIN UNIDADMEDIDAARTICULO UM ON PA.IDUNIDADVENTA \= UM.ID\nINNER JOIN PROPUESTAARTICULOPRECIO PAP ON PA.ID \= PAP.IDPROPUESTAARTICULO\nLEFT JOIN SUCURSAL SC ON PAP.IDCLUSTER \= SC.IDCLUSTER\nLEFT JOIN SUCURSAL SF ON PAP.IDFORMATO \= SF.IDFORMATO\nLEFT JOIN SUCURSAL S ON PAP.IDSUCURSAL \= S.ID\nWHERE PA.ID \= ?&#xD;
joincount=0&#xD;
tablecount=0&#xD;
</wizardData>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>vendorNumber</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
                <oraObjects/>
                <oraTables/>
                <QueryOutputCachedSchemaColumns>VPC_ALW_SEQ</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>number</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            </config>
            <pd:inputBindings>
                <jdbcQueryActivityInput>
                    <vendorNumber>
                        <xsl:value-of select="$Start/vendor_number"/>
                    </vendorNumber>
                </jdbcQueryActivityInput>
            </pd:inputBindings>
        </pd:activity>
