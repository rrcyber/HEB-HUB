            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>286</pd:x>
            <pd:y>373</pd:y>
            <config/>
            <pd:inputBindings>
