                            </pfx6:sev_id>
                        </pfx6:ErrorManagementSchema>
                    </pfx6:Body>
                </pfx6:MSG_Container>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Retry on error</pd:to>
