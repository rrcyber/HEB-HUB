        REPLACE(REPLACE(REPLACE(REPLACE(TRANSLATE(I.DESCR,'ñáéíóúàèìòùãõâêîôôäëïöüçÑÿÉÿÓÚÀÈÌÒÙÃÕÂÊÎÔÛÄËÿÖÜÇ-','naeiouaeiouaoaeiooaeioucNAEIOUAEIOUAOAEIOOAEIOUC '),' (ASI)',''),' (CAM)',''),'(ASI)',''),'(CAM)','')                "LOCATION_DESCR",
        TRANSLATE(I.CITY,'ñáéíóúàèìòùãõâêîôôäëïöüçÑÿÉÿÓÚÀÈÌÒÙÃÕÂÊÎÔÛÄËÿÖÜÇ-','naeiouaeiouaoaeiooaeioucNAEIOUAEIOUAOAEIOOAEIOUC ') "CITY",
        TRUNC(A.ACTION_DT)                                        "LAST_UPDATE_DT",
        A.GP_PAYGROUP                                  "TIPO_DE_CONTRATO",
		A.STD_HOURS                                      "STD_HOURS",
        MANAGER.EMPLID  "MANAGER_ID"
FROM  PS_JOB              A
JOIN  PS_PERSONAL_DATA    B ON A.EMPLID = B.EMPLID
LEFT
JOIN  PS_PERSONAL_PHONE   C ON A.EMPLID = C.EMPLID AND C.PHONE_TYPE = 'WORK'
LEFT
JOIN  PS_PERS_NID         D ON A.EMPLID = D.EMPLID AND D.NATIONAL_ID_TYPE  = 'IMSS'
LEFT
JOIN  PS_EMAIL_ADDRESSES  E ON A.EMPLID = E.EMPLID AND E_ADDR_TYPE = 'BUSN'
JOIN  PS_DEPT_TBL         F ON A.DEPTID = F.DEPTID
JOIN  PS_JOBCODE_TBL      G ON A.JOBCODE = G.JOBCODE
LEFT
JOIN  PS_PERS_NID         H ON A.EMPLID = H.EMPLID AND H.NATIONAL_ID_TYPE  = 'RFC'
JOIN  PS_LOCATION_TBL     I ON A.LOCATION = I.LOCATION
JOIN  PS_COMPANY_TBL      J ON  A.COMPANY = J.COMPANY
OUTER APPLY (
  SELECT B.EMPLID,
         B.POSITION_ENTRY_DT
	FROM	  PS_JOB B
  JOIN    PS_PERSONAL_DATA    C ON B.EMPLID = C.EMPLID
  WHERE		B.POSITION_NBR = A.REPORTS_TO
		  AND B.EFFDT = (SELECT MAX(C.EFFDT) FROM PS_JOB C WHERE B.EMPLID = C.EMPLID)
		  AND B.EFFSEQ = (SELECT MAX(D.EFFSEQ) FROM PS_JOB D WHERE B.EMPLID = D.EMPLID AND B.EFFDT = D.EFFDT)
		  AND B.HR_STATUS='A'  
      AND B.POSITION_ENTRY_DT =(SELECT MAX(E.POSITION_ENTRY_DT) FROM PS_JOB  E  WHERE B.POSITION_NBR = E.POSITION_NBR)
	) MANAGER
WHERE     A.EFFDT   =   (SELECT MAX(K.EFFDT)  FROM  PS_JOB                        K WHERE A.EMPLID    = K.EMPLID     )
      AND A.EFFSEQ  =   (SELECT MAX(L.EFFSEQ) FROM  PS_JOB          L WHERE A.EMPLID    = L.EMPLID    AND A.EFFDT       = L.EFFDT)
      AND G.EFFDT   =   (SELECT MAX(M.EFFDT)  FROM  PS_JOBCODE_TBL M WHERE G.JOBCODE    = M.JOBCODE   AND M.EFF_STATUS = 'A')
      AND I.EFFDT   =   (SELECT MAX(N.EFFDT)  FROM  PS_LOCATION_TBL N WHERE I.LOCATION  = N.LOCATION  AND N.EFF_STATUS  = 'A')
      AND J.EFFDT   =   (SELECT MAX(O.EFFDT)  FROM  PS_COMPANY_TBL  O WHERE J.COMPANY   = O.COMPANY   AND O.EFF_STATUS  = 'A')
      AND A.ACTION          IN  ('HIR','REH','TER','XFR','CTC','POS','PRO')  
      AND (
            (
              TRUNC(A.EFFDT)       = TRUNC(SYSDATE-?)
           )
            OR
            (
              TRUNC(A.ACTION_DT)  = TRUNC(SYSDATE-?)
              AND
              TRUNC(A.EFFDT)        &lt;= TRUNC(SYSDATE-?)
            )
          )  
ORDER BY  TIPO_DE_MOVIMIENTO,EFF_DT,TO_NUMBER(A.EMPLID)
</statement>
