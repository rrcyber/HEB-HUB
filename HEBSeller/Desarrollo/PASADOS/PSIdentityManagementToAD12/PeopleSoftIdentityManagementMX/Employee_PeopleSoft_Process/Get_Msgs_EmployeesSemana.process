            <statement>Select a.EMPL_ID,a.INSERTEDDATE_DETAIL,a.INSERTEDDATE ,a.Resultado_AD,a.Operacion_AD ,a.AD_Msg_Result,b.name as display_name,b.Location_id,b.Dept_id,b.Operacion_PS,
 b.EFF_DT,b.LOCATION_DESCR,b.JOB_CD,b.JOBCODE_DESCR,b.Tipo_Contrato,b.NATIONAL_ID ,DATEADD(day,-(?),CONVERT (date, ?,126) ) as inicial,DATEADD(day,-(1),CONVERT (date, ?,126) ) as final
 from Control_PS_AD_MX a,PS_EMPL_DATA_DETAIL_MX b 
 where a.EMPL_ID=b.EMPL_ID and a.INSERTEDDATE_DETAIL=b.INSERTEDDATE  
 and (CONVERT (date,a.INSERTEDDATE,126) &gt;=DATEADD(day,-(?),CONVERT (date, ?,126) ))
 and (CONVERT (date,a.INSERTEDDATE,126) &lt;CONVERT (date, ?,126) )
union all
 Select c.EMPL_ID,c.INSERTEDDATE,c.INSERTEDDATE ,c.Resultado_AD,c.Operacion_AD ,'Error AD' as AD_Msg_Result,c.name as display_name,c.Location_id,c.Dept_id,c.Operacion_PS,
 c.EFF_DT,c.LOCATION_DESCR,c.JOB_CD,c.JOBCODE_DESCR,c.Tipo_Contrato,c.NATIONAL_ID ,DATEADD(day,-(0),CONVERT (date, c.INSERTEDDATE,126) ) as inicial,DATEADD(day,-(0),CONVERT (date, c.INSERTEDDATE,126) ) as final
 from PS_EMPL_DATA_DETAIL_MX c where Status_Ad='Z'
 order by INSERTEDDATE_DETAIL,Location_id,Dept_id,Operacion_PS
 </statement>
