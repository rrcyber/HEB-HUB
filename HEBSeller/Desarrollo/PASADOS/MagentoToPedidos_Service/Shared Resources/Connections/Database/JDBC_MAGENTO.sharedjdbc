<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>JDBC_MAGENTO</name>
    <resourceType>ae.shared.JDBCSharedResource</resourceType>
    <config>
        <driver>com.mysql.jdbc.Driver</driver>
        <maxConnections>%%Connections/Database/MAGENTO/GLB_MAX_CONNECTIONS%%</maxConnections>
        <loginTimeout>%%Connections/Database/MAGENTO/GLB_TIMEOUT%%</loginTimeout>
        <connectionType>JDBC</connectionType>
        <UseSharedJndiConfig>false</UseSharedJndiConfig>
        <location>%%Connections/Database/MAGENTO/MIG_DATABASE_URL%%</location>
        <user>%%Connections/Database/MAGENTO/MIG_DBUSERNAME%%</user>
        <password>%%Connections/Database/MAGENTO/MIG_DBPASSWORD%%</password>
    </config>
</BWSharedResource>