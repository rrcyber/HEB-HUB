<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="www.tibco.com/plugin/Sleep" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns3="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://xmlns.heb.com/ei/CatalogoPlannerList" xmlns:pfx2="http://www.tibco.com/ns/no_namespace_schema_location/Shared Resources/Schema Definitions/Common/Exception.xsd" xmlns:ns12="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx="http://xmlns.heb.com/ei/error">
    <xsd:import namespace="http://www.tibco.com/ns/no_namespace_schema_location/Shared Resources/Schema Definitions/Common/Exception.xsd" schemaLocation="/Shared Resources/Schema Definitions/Common/Exception.xsd"/>
    <xsd:import namespace="http://xmlns.heb.com/ei/CatalogoPlannerList" schemaLocation="/Shared Resources/Schema Definitions/Custom/CATALOGO_PLANNER_LIST.xsd"/>
    <pd:name>Heb2bOrdenCompraReciboSubscriber/Sub Process/Distribution_Thread_Main.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Input">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="PurchaseOrder" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>37</pd:startX>
    <pd:startY>232</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>796</pd:endX>
    <pd:endY>230</pd:endY>
    <pd:errorSchemas>
        <Exception ref="pfx2:Exception"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1448660298450</pd:targetNamespace>
    <pd:group name="Retry Group on Error">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>218</pd:x>
        <pd:y>30</pd:y>
        <pd:width>362</pd:width>
        <pd:height>385</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>count</pd:indexSlot>
            <pd:errorCondition>$count &gt;= $_globalVariables/ns:GlobalVariables/RetryConfig/GLB_MAX_RETRIES</pd:errorCondition>
            <pd:suspendAfterErrorRetry>true</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Planner_Thread</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Validate Error</pd:from>
            <pd:to>end</pd:to>
            <pd:xpathDescription>Is Error Data</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$Validate-Error/ErrorCode='true'</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Planner_Thread</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>One Bend</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>ErrorProcess</pd:to>
            <pd:xpathDescription>Count =3</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$count = $_globalVariables/ns:GlobalVariables/RetryConfig/GLB_MAX_RETRIES</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Generate-Error-1</pd:to>
            <pd:xpathDescription>No</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Catch</pd:from>
            <pd:to>Validate Error</pd:to>
            <pd:xpathDescription>Is Error</pd:xpathDescription>
            <pd:lineType>One Bend</pd:lineType>
            <pd:lineColor>-3407872</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Validate Error</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:xpathDescription>Is Technical Error</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:activity name="Planner_Thread">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>306</pd:x>
            <pd:y>93</pd:y>
            <config>
                <processName>/Heb2bOrdenCompraReciboSubscriber/Sub Process/GetDistributionList.process</processName>
            </config>
            <pd:inputBindings>
                <Input>
                    <PurchaseOrder>
                        <xsl:value-of select="$Start/Input/PurchaseOrder"/>
                    </PurchaseOrder>
                </Input>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>393</pd:x>
            <pd:y>335</pd:y>
            <config/>
            <pd:inputBindings>
                <ns1:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/RetryConfig/GBL_RETRY_DELAY"/>
                    </IntervalInMillisec>
                </ns1:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="ErrorProcess">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>493</pd:x>
            <pd:y>335</pd:y>
            <config>
                <processName>/Utility Services/Exception Handling/ErrorProcess.process</processName>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$_error_Planner_Thread/Exception"/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Catch">
            <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
            <pd:resourceType>ae.activities.catch</pd:resourceType>
            <pd:x>314</pd:x>
            <pd:y>171</pd:y>
            <pd:handler>true</pd:handler>
            <config>
                <catchAll>true</catchAll>
            </config>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="Generate-Error-1">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>299</pd:x>
            <pd:y>336</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns12:ActivityInput>
                    <message>
                        <xsl:value-of select="$_error/ns2:ErrorReport/Msg"/>
                    </message>
                    <messageCode>
                        <xsl:value-of select="$_error/ns2:ErrorReport/MsgCode"/>
                    </messageCode>
                </ns12:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Validate Error">
            <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
            <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
            <pd:x>390</pd:x>
            <pd:y>227</pd:y>
            <config>
                <element>
                    <xsd:element name="ErrorCode" type="xsd:string"/>
                </element>
            </config>
            <pd:inputBindings>
                <ErrorCode>
                    <xsl:value-of select="contains($_globalVariables/ns:GlobalVariables/ExceptionHandling/GLB_CONDITION_FOR_SUSPEND, $_error/ns2:ErrorReport/MsgCode)"/>
                </ErrorCode>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>404</pd:x>
        <pd:y>491</pd:y>
        <config>
            <faultName>Exception</faultName>
        </config>
        <pd:inputBindings>
            <ns12:ActivityInput>
                <message>
                    <xsl:value-of select="$_error/ns2:ErrorReport/Msg"/>
                </message>
                <messageCode>
                    <xsl:value-of select="$_error/ns2:ErrorReport/MsgCode"/>
                </messageCode>
                <data>
                    <xsl:copy-of select="$_error_Planner_Thread/Exception"/>
                </data>
            </ns12:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch-1">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>687</pd:x>
        <pd:y>463</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <fault>localname=Exception</fault>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="ErrorProcess-1">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>796</pd:x>
        <pd:y>464</pd:y>
        <config>
            <processName>/Utility Services/Exception Handling/ErrorProcess.process</processName>
        </config>
        <pd:inputBindings>
            <xsl:copy-of select="$Catch-1/Exception"/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="calculateThread">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>144</pd:x>
        <pd:y>233</pd:y>
        <config>
            <processName>/Heb2bOrdenCompraReciboSubscriber/Sub Process/CalculateThread.process</processName>
        </config>
        <pd:inputBindings>
            <group>
                <operation>
                    <xsl:value-of xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="'SUM'"/>
                </operation>
            </group>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="calculateThread-1">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>676</pd:x>
        <pd:y>230</pd:y>
        <config>
            <processName>/Heb2bOrdenCompraReciboSubscriber/Sub Process/CalculateThread.process</processName>
        </config>
        <pd:inputBindings>
            <group>
                <operation>
                    <xsl:value-of xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="'SUBS'"/>
                </operation>
            </group>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="calculateThread-1-1">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>796</pd:x>
        <pd:y>346</pd:y>
        <config>
            <processName>/Heb2bOrdenCompraReciboSubscriber/Sub Process/CalculateThread.process</processName>
        </config>
        <pd:inputBindings>
            <group>
                <operation>
                    <xsl:value-of xmlns:xsl="http://www.w3.org/1999/XSL/Transform" select="'SUBS'"/>
                </operation>
            </group>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Retry Group on Error</pd:from>
        <pd:to>calculateThread-1</pd:to>
        <pd:xpathDescription>No Error</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>not(exists($_error))</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Retry Group on Error</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:xpathDescription>Error</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65485</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch-1</pd:from>
        <pd:to>ErrorProcess-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ErrorProcess-1</pd:from>
        <pd:to>calculateThread-1-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>calculateThread</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>calculateThread</pd:from>
        <pd:to>Retry Group on Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>calculateThread-1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>calculateThread-1-1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>