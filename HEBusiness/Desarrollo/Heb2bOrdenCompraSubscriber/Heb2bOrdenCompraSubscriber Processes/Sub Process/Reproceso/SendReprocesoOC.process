                                        <xsd:sequence>
                                            <xsd:element name="Articulo" type="xsd:string"/>
                                            <xsd:element name="Casepack" type="xsd:string"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
