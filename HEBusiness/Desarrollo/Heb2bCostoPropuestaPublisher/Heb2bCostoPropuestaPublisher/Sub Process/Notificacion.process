<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:BW="java://com.tibco.pe.core.JavaCustomXPATHFunctions" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns14="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns12="http://xmlns.heb.com/ei/ErrorManagementSchema" xmlns:ns13="www.tibco.com/plugin/Sleep" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <pd:name>Heb2bCostoPropuestaPublisher/Sub Process/Notificacion.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="IdPropuesta1" type="xsd:string"/>
                    <xsd:element name="IdEstatus" type="xsd:string"/>
                    <xsd:element name="IdPropuesta" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>27</pd:startX>
    <pd:startY>188</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>832</pd:endX>
    <pd:endY>188</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1419868656819</pd:targetNamespace>
    <pd:activity name="insertNotificacion">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>663</pd:x>
        <pd:y>186</pd:y>
        <config>
            <processName>/Heb2bCostoPropuestaPublisher/Sub Process/insertNotificacion.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <IdEstatus>
                    <xsl:value-of select="$Start/root/IdEstatus"/>
                </IdEstatus>
                <Idpropuesta>
                    <xsl:value-of select="$Start/root/IdPropuesta"/>
                </Idpropuesta>
                <IdDestinatario>
                    <xsl:value-of select="$GetCasePackDestinatario/resultSet/Record[1]/usuariomodificacion"/>
                </IdDestinatario>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Retry on error">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>144</pd:x>
        <pd:y>1</pd:y>
        <pd:width>370</pd:width>
        <pd:height>355</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:errorCondition>$i &gt;= $_globalVariables/ns1:GlobalVariables/Heb2bCostoPropuestaPublisherVariables/GLB_MAX_RETRIES</pd:errorCondition>
            <pd:suspendAfterErrorRetry>true</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>GetCasePackDestinatario</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>GetCasePackDestinatario</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>GetCasePackDestinatario</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-52429</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>JDBC Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Error ProcessBD</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$i = $_globalVariables/ns1:GlobalVariables/Heb2bCostoPropuestaPublisherVariables/GLB_MAX_RETRIES</pd:xpath>
        </pd:transition>
        <pd:activity name="GetCasePackDestinatario">
            <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
            <pd:x>328</pd:x>
            <pd:y>64</pd:y>
            <config>
                <timeout>%%Connections/Database/JDBC_HEBB/GLB_TIMEOUT%%</timeout>
                <commit>false</commit>
                <maxRows>%%Connections/Database/JDBC_HEBB/GLB_MAX_ROWS%%</maxRows>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>select usuariomodificacion from dbo.PropuestaCostoArticulo where id = ?</statement>
                <jdbcSharedConfig>/Shared Resources/Connections/Database/JDBC_HEBB.sharedjdbc</jdbcSharedConfig>
                <wizardData>#Sat Feb 08 19:01:48 CST 2014&#xD;
useSchemaInFQN=true&#xD;
columncount=0&#xD;
manualSQLText=SELECT UM.IDPMM UNIDADVENTA, PAP.PRECIOVENTASUGERIDO,\n\t\tCASE WHEN PAP.IDFORMATO IS NOT NULL THEN SF.IDPMM\n\t\t\tWHEN PAP.IDCLUSTER IS NOT NULL THEN SC.IDPMM\n\t\t\tELSE S.IDPMM\n\t\tEND AS IDSUCURSAL,\n\t\t(SELECT F.IDPMM FROM FORMATO F\n\t\t\tINNER JOIN SUCURSAL S ON F.ID \= S.IDFORMATO WHERE S.IDPMM \= IDSUCURSAL) IDFORMATO\nFROM PROPUESTAARTICULO PA\nINNER JOIN UNIDADMEDIDAARTICULO UM ON PA.IDUNIDADVENTA \= UM.ID\nINNER JOIN PROPUESTAARTICULOPRECIO PAP ON PA.ID \= PAP.IDPROPUESTAARTICULO\nLEFT JOIN SUCURSAL SC ON PAP.IDCLUSTER \= SC.IDCLUSTER\nLEFT JOIN SUCURSAL SF ON PAP.IDFORMATO \= SF.IDFORMATO\nLEFT JOIN SUCURSAL S ON PAP.IDSUCURSAL \= S.ID\nWHERE PA.ID \= ?&#xD;
joincount=0&#xD;
tablecount=0&#xD;
</wizardData>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>IdPrpuesta</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
                <oraObjects/>
                <oraTables/>
                <QueryOutputCachedSchemaColumns>usuariomodificacion</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>int</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            </config>
            <pd:inputBindings>
                <jdbcQueryActivityInput>
                    <IdPrpuesta>
                        <xsl:value-of select="$Start/root/IdPropuesta"/>
                    </IdPrpuesta>
                </jdbcQueryActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>329</pd:x>
            <pd:y>279</pd:y>
            <config/>
            <pd:inputBindings>
                <ns13:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Heb2bCostoPropuestaPublisherVariables/GLB_RETRAY_DELAY"/>
                    </IntervalInMillisec>
                </ns13:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="JDBC Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>435</pd:x>
            <pd:y>279</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns14:ActivityInput>
                    <message>
                        <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                    </message>
                    <xsl:if test="$_error/ns:ErrorReport/MsgCode">
                        <messageCode>
                            <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                        </messageCode>
                    </xsl:if>
                </ns14:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Error ProcessBD">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>218</pd:x>
            <pd:y>279</pd:y>
            <config>
                <processName>/Utility Services/Exception Handling/ErrorProcess.process</processName>
            </config>
            <pd:inputBindings>
                <ns12:MSG_Container>
                    <ns12:Header>
                        <ns12:ProjectName>
                            <xsl:value-of select="$_processContext/ns:ProcessContext/ProjectName"/>
                        </ns12:ProjectName>
                        <ns12:Source_Domain>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ProjectConfig/Source_Domain"/>
                        </ns12:Source_Domain>
                        <ns12:Destination_Domain>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ProjectConfig/Destination_Domain"/>
                        </ns12:Destination_Domain>
                        <ns12:Transaction_Type>
                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ProjectConfig/Transaction_Type"/>
                        </ns12:Transaction_Type>
                        <ns12:TimeStamp>
                            <xsl:value-of select="tib:format-dateTime(&quot;yyyy-MM-dd hh:mm:ss&quot;, current-dateTime())"/>
                        </ns12:TimeStamp>
                    </ns12:Header>
                    <ns12:Body>
                        <ns12:ErrorManagementSchema>
                            <ns12:inrtn_id>
                                <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ExceptionHandling/MIG_ERR_INTEGRATION_ID"/>
                            </ns12:inrtn_id>
                            <ns12:inrtn_instn_nm>
                                <xsl:value-of select="BW:getCurrentProcessName($_processContext/ns:ProcessContext/ProcessId)"/>
                            </ns12:inrtn_instn_nm>
                            <ns12:inrtn_err_rtn_desc>
                                <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                            </ns12:inrtn_err_rtn_desc>
                            <ns12:inrtn_msg_body>
                                <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                            </ns12:inrtn_msg_body>
                            <ns12:cre8_ts>
                                <xsl:value-of select="tib:format-dateTime(&quot;yyyy-MM-dd hh:mm:ss&quot;, current-dateTime())"/>
                            </ns12:cre8_ts>
                            <ns12:u_code>
                                <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                            </ns12:u_code>
                            <ns12:sev_id>
                                <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ExceptionHandling/GLB_ERR_SEV_HIGH"/>
                            </ns12:sev_id>
                        </ns12:ErrorManagementSchema>
                    </ns12:Body>
                </ns12:MSG_Container>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Retry on error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Retry on error</pd:from>
        <pd:to>insertNotificacion</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>insertNotificacion</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>