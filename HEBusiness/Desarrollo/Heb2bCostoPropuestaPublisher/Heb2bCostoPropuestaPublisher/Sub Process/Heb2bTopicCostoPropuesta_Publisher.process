                    <statement>SELECT 
IDDETALLE, 
PROVEEDOR,
CASEPACK, 
SKU, 
ID, 
FECHAINICIO, 
COSTOCASEPACK , 
SUCURSAL, 
FECHAINICIODETALLE
from (
SELECT 
PCD.ID IDDETALLE, 
P.IDPMM PROVEEDOR, 
CP.IDPMM CASEPACK, 
A.SKU as sku , 
PCA.ID as id,
PCA.FECHAINICIO as fechainicio ,
PCD.COSTOCASEPACK AS COSTOCASEPACK , 
null SUCURSAL,
FECHAINICIODETALLE =
CASE WHEN PCD.FECHAINICIO IS NULL
			THEN CASE WHEN CONVERT(VARCHAR(8), PCA.FECHAINICIO, 112) &gt; CONVERT(varchar(8), GETDATE(),112)
					THEN PCA.FECHAINICIO
					ELSE getdate()+1
					END
			ELSE CASE WHEN CONVERT(VARCHAR(8), PCD.FECHAINICIO, 112) &gt; CONVERT(varchar(8), GETDATE(),112)
					THEN PCD.FECHAINICIO
					ELSE getdate()+1
					END  
      END
FROM PROPUESTACOSTOARTICULO PCA
JOIN PROVEEDOR P ON PCA.IDPROVEEDOR = P.ID
JOIN PROPUESTACOSTOARTICULODETALLE PCD ON PCA.ID = PCD.IDPROPUESTACOSTO
JOIN CASEPACK CP ON CP.ID = PCD.IDCASEPACK
JOIN ARTICULO A ON CP.IDARTICULO = A.ID
WHERE CP.ESACTIVO = 1 AND PCA.ID = ? AND PCD.COSTOCASEPACK IS NOT NULL AND PCD.IDESTATUS = 9
union
SELECT 
PCD.ID IDDETALLE, 
P.IDPMM PROVEEDOR, 
CP.IDPMM CASEPACK, 
A.SKU, 
PCA.ID, 
PCA.FECHAINICIO,
PCDS.COSTOCASEPACK AS COSTOCASEPACK,
S.IDPMM SUCURSAL, 
FECHAINICIODETALLE =
CASE WHEN PCD.FECHAINICIO IS NULL
			THEN CASE WHEN CONVERT(VARCHAR(8), PCA.FECHAINICIO, 112) &gt; CONVERT(varchar(8), GETDATE(),112)
					THEN PCA.FECHAINICIO
					ELSE getdate()+1
					END
			ELSE CASE WHEN CONVERT(VARCHAR(8), PCD.FECHAINICIO, 112) &gt; CONVERT(varchar(8), GETDATE(),112)
					THEN PCD.FECHAINICIO
					ELSE getdate()+1
					END  
      END
FROM PROPUESTACOSTOARTICULO PCA
JOIN PROVEEDOR P ON PCA.IDPROVEEDOR = P.ID
JOIN PROPUESTACOSTOARTICULODETALLE PCD ON PCA.ID = PCD.IDPROPUESTACOSTO
JOIN CASEPACK CP ON CP.ID = PCD.IDCASEPACK
JOIN ARTICULO A ON CP.IDARTICULO = A.ID
LEFT JOIN PROPUESTACOSTOARTICULODETALLESUCURSAL PCDS ON PCD.ID = PCDS.IDPROPUESTACOSTOARTICULODETALLE
LEFT JOIN SUCURSAL S ON PCDS.IDSUCURSAL = S.ID
WHERE CP.ESACTIVO = 1 AND  PCA.ID = ? AND PCD.IDESTATUS = 9 AND PCDS.COSTOCASEPACK IS NOT NULL)a
order by casepack , sucursal</statement>
