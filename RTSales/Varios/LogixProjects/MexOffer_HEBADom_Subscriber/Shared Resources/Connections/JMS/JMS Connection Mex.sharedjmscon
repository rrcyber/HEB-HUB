<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>JMS Connection Mex</name>
    <resourceType>ae.shared.JMSConnectionKey</resourceType>
    <config>
        <NamingEnvironment>
            <UseJNDI>true</UseJNDI>
            <ProviderURL>%%Connections/JMS/EMSMEX/MEX_JMSPROVIDERURL%%</ProviderURL>
            <NamingURL>%%Connections/JMS/EMSMEX/MEX_JMSPROVIDERURL%%</NamingURL>
            <NamingInitialContextFactory>com.tibco.tibjms.naming.TibjmsInitialContextFactory</NamingInitialContextFactory>
            <TopicFactoryName>%%Connections/JMS/EMS/TopicConnectionFactory%%</TopicFactoryName>
            <QueueFactoryName>%%Connections/JMS/EMS/QueueConnectionFactory%%</QueueFactoryName>
            <NamingPrincipal>%%Connections/JMS/EMSMEX/MEX_JNDI_USERNAME%%</NamingPrincipal>
            <NamingCredential>%%Connections/JMS/EMSMEX/MEX_JNDI_PASSWORD%%</NamingCredential>
        </NamingEnvironment>
        <ConnectionAttributes>
            <username>%%Connections/JMS/EMSMEX/MEX_EMS_USERNAME%%</username>
            <password>%%Connections/JMS/EMSMEX/MEX_EMS_PASSWORD%%</password>
            <clientID>%%Connections/JMS/EMSMEX/MEX_EMS_CLIENTID%%</clientID>
            <autoGenClientID>true</autoGenClientID>
        </ConnectionAttributes>
        <UseXACF>false</UseXACF>
    </config>
</BWSharedResource>