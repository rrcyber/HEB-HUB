							<xs:element name="StoredValue" type="StoredValueType" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Auxilary" type="AuxilaryType" minOccurs="0"/>
				<xs:element name="Banners" type="BannerOffers" minOccurs="0"/>
