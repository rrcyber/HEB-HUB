                    <xsd:sequence>
                        <xsd:element name="RESPONSE_BACK" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:schema>
        <xsd:schema xmlns="http://xmlns.heb.com/ei/SHEET_DIFFERENCES_RESPONSE" xmlns:xsd="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://xmlns.heb.com/ei/SHEET_DIFFERENCES_RESPONSE">
            <xsd:complexType name="DETAIL">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="PRODUCT_ID" type="xsd:int"/>
                    <xsd:element minOccurs="0" name="BAR_CODE" type="xsd:string"/>
