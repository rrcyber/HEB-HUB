<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:ns16="http://www.sat.gob.mx/cfd/3" xmlns:ns17="http://www.sat.gob.mx/psgecfd" xmlns:ns14="http://www.sat.gob.mx/cfd/2" xmlns:ns15="http://www.sat.gob.mx/detallista" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:ns9="http://xmlns.heb.com/ei/RECEIPT_NUMBER_REQUEST" xmlns:ns5="http://xmlns.heb.com/ei/DIGITAL_INVOICE_REQUEST" xmlns:ns12="http://xmlns.heb.com/ei/MESSAGE_REPLY" xmlns:ns6="http://xmlns.heb.com/ei/DIGITAL_INVOICE_RESPONSE" xmlns:ns13="http://www.sat.gob.mx/TimbreFiscalDigital" xmlns:ns7="http://xmlns.heb.com/ei/PURCHASE_ORDER_REQUEST" xmlns:ns10="http://xmlns.heb.com/ei/RECEIPT_NUMBER_RESPONSE" xmlns:ns8="http://xmlns.heb.com/ei/PURCHASE_ORDER_RESPONSE" xmlns:ns11="http://xmlns.heb.com/ei/Message_Request" xmlns:ns2="http://xmlns.heb.com/ei/SPGFaultSchema" xmlns:ns1="http://xmlns.heb.com/ei/INTEGRATION_NUMBER_RESPONSE" xmlns:ns4="http://xmlns.heb.com/ei/SHEET_DIFFERENCES_RESPONSE" xmlns:ns3="http://xmlns.heb.com/ei/SHEET_DIFFERENCES_REQUEST" xmlns:tns="http://www.heb.com/ei/MexicoDigitalInvoiceService" xmlns:ns0="http://xmlns.heb.com/ei/INTEGRATION_NUMBER_REQUEST" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/" name="Untitled" targetNamespace="http://www.heb.com/ei/MexicoDigitalInvoiceService">
    <wsdl:types>
        <xsd:schema xmlns="http://xmlns.heb.com/ei/DIGITAL_INVOICE_REQUEST" xmlns:detallista="http://www.sat.gob.mx/detallista" xmlns:cfdi="http://www.sat.gob.mx/cfd/3" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:psgecfd="http://www.sat.gob.mx/psgecfd" xmlns:tfd="http://www.sat.gob.mx/TimbreFiscalDigital" xmlns:cfd="http://www.sat.gob.mx/cfd/2" attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="http://xmlns.heb.com/ei/DIGITAL_INVOICE_REQUEST">
