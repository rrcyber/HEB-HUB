                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element maxOccurs="5" ref="cfdi:Comprobante">
                                <xsd:annotation>
                                    <xsd:documentation>Estándar de Comprobante fiscal digital a través de Internet.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:element>
                        </xsd:choice>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:schema>
        <xsd:schema xmlns="http://xmlns.heb.com/ei/INTEGRATION_NUMBER_REQUEST" xmlns:xsd="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://xmlns.heb.com/ei/INTEGRATION_NUMBER_REQUEST">
            <xsd:element name="GET_INTEGRATION_NUMBER_REQUEST">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="ISBUREAU" type="xsd:int"/>
                        <xsd:choice>
                            <xsd:element name="RFC" nillable="true" type="ns0:RFC_Type"/>
                            <xsd:element name="VENDOR_NUMBER" nillable="true" type="xsd:int"/>
                        </xsd:choice>
                        <xsd:element minOccurs="0" name="INVOICE_NUMBER" nillable="true" type="xsd:string"/>
                        <xsd:element minOccurs="0" name="RECEIPT_NUMBER" nillable="true" type="xsd:long"/>
