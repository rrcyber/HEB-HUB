                <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
                <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
                <pd:x>414</pd:x>
                <pd:y>130</pd:y>
                <config>
                    <timeout>%%Connections/Database/DatabaseName/GLB_TIMEOUT%%</timeout>
                    <commit>false</commit>
                    <emptyStrAsNil>false</emptyStrAsNil>
                    <statement>UPDATE SDITRFDTI SET SDIFLAG = 1
WHERE SDIFLAG = 0
AND CARTON_NUMBER = ?
AND ACTION_CODE = '08'</statement>
                    <jdbcSharedConfig>/Shared Resources/Connections/Database/JDBC-Connection-BDDIS.sharedjdbc</jdbcSharedConfig>
                    <Prepared_Param_DataType>
                        <parameter>
                            <parameterName>FOLIO</parameterName>
                            <dataType>VARCHAR</dataType>
                        </parameter>
                    </Prepared_Param_DataType>
                </config>
                <pd:inputBindings>
                    <jdbcUpdateActivityInput>
                        <FOLIO>
                            <xsl:value-of select="$Start/root/FOLIO"/>
                        </FOLIO>
                    </jdbcUpdateActivityInput>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="JDBC Update Bitacora">
                <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
                <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
                <pd:x>417</pd:x>
                <pd:y>282</pd:y>
                <config>
                    <timeout>%%Connections/Database/DatabaseName/GLB_TIMEOUT%%</timeout>
                    <commit>false</commit>
                    <emptyStrAsNil>false</emptyStrAsNil>
                    <jdbcSharedConfig>/Shared Resources/Connections/Database/JDBC Connection.sharedjdbc</jdbcSharedConfig>
                    <statement>UPDATE MERC_BitacoraEventos SET sdi_flag = 1
WHERE folio = ?
AND sucursal = ?
AND sdi_flag = 0</statement>
                    <Prepared_Param_DataType>
                        <parameter>
