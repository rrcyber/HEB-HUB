import java.util.*;
import java.io.*;
public class DistributionList_StarterJavaCode{
/****** START SET/GET METHOD, DO NOT MODIFY *****/
	protected String ERROR_CODE = "";
	protected String ERRORS = "";
	protected String TYPE_ERROR = "";
