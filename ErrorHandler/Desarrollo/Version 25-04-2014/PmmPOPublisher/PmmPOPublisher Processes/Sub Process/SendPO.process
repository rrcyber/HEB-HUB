        <pd:type>com.tibco.plugin.jms.JMSTopicPublishActivity</pd:type>
        <pd:resourceType>ae.activities.JMSTopicPublishActivity</pd:resourceType>
        <pd:description>Purchase Order</pd:description>
        <pd:x>1101</pd:x>
        <pd:y>302</pd:y>
        <config>
            <PermittedMessageType>XML Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>1</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>%%Destinations/Topics/MIG_PURCHASEORDER_TOPIC%%</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/Shared Resources/Connections/JMS/JMS Connection.sharedjmscon</ConnectionReference>
            <InDataxsdString ref="pfx:MSG_CONTAINER"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <JMSCorrelationID>
                    <xsl:value-of select="'PO'"/>
                </JMSCorrelationID>
                <Body>
                    <pfx:MSG_CONTAINER>
                        <pfx:Header>
                            <pfx:ProjectName>
                                <xsl:value-of select="$_processContext/ns2:ProcessContext/ProjectName"/>
                            </pfx:ProjectName>
                            <pfx:Source_Domain>
                                <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ProjectConfig/Source_Domain"/>
                            </pfx:Source_Domain>
                            <pfx:Destination_Domain>
                                <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ProjectConfig/Destination_Domain"/>
                            </pfx:Destination_Domain>
                            <pfx:Transaction_Type>
                                <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/ProjectConfig/Transaction_Type"/>
                            </pfx:Transaction_Type>
                            <pfx:TimeStamp>
                                <xsl:value-of select="tib:format-dateTime(&quot;yyyy-MM-dd hh:mm:ss&quot;, current-dateTime())"/>
                            </pfx:TimeStamp>
                        </pfx:Header>
                        <pfx:Body>
                            <xsl:for-each select="$JDBC-Query-Header/resultSet/Record">
                                <pfx:PURCHASE_ORDER>
                                    <pfx:PRIM_ORG_LVL_NUMBER>
                                        <xsl:value-of select="tib:trim(PRIM_ORG_LVL_NUMBER)"/>
                                    </pfx:PRIM_ORG_LVL_NUMBER>
                                    <pfx:PMG_PO_NUMBER>
                                        <xsl:value-of select="tib:trim(PMG_PO_NUMBER)"/>
                                    </pfx:PMG_PO_NUMBER>
                                    <pfx:PMG_STAT_CODE>
                                        <xsl:value-of select="tib:trim($Start/group/PMG_STAT_CODE)"/>
                                    </pfx:PMG_STAT_CODE>
                                    <pfx:VENDOR_NUMBER>
                                        <xsl:value-of select="tib:trim(VENDOR_NUMBER)"/>
                                    </pfx:VENDOR_NUMBER>
                                    <pfx:PMG_RELEASE_DATE>
                                        <xsl:value-of select="tib:trim(PMG_RELEASE_DATE)"/>
                                    </pfx:PMG_RELEASE_DATE>
                                    <pfx:PMG_ENTRY_DATE>
                                        <xsl:value-of select="tib:trim(PMG_ENTRY_DATE)"/>
                                    </pfx:PMG_ENTRY_DATE>
                                    <pfx:CURR_CODE>
                                        <xsl:value-of select="tib:trim(CURR_CODE)"/>
                                    </pfx:CURR_CODE>
                                    <pfx:PMG_SHORT_CODE>
                                        <xsl:value-of select="tib:trim(PMG_SHORT_CODE)"/>
                                    </pfx:PMG_SHORT_CODE>
                                    <pfx:PMG_USER>
                                        <xsl:value-of select="tib:trim(PMG_USER)"/>
                                    </pfx:PMG_USER>
                                    <pfx:VPC_PO_PROC_TIME>
                                        <xsl:value-of select="tib:trim(VPC_PO_PROC_TIME)"/>
                                    </pfx:VPC_PO_PROC_TIME>
                                    <pfx:VPC_PO_SHIP_TIME>
                                        <xsl:value-of select="tib:trim(VPC_PO_SHIP_TIME)"/>
                                    </pfx:VPC_PO_SHIP_TIME>
                                    <pfx:PMG_PO_NOTES>
                                        <xsl:value-of select="tib:trim(PMG_PO_NOTES)"/>
                                    </pfx:PMG_PO_NOTES>
                                    <pfx:PMG_RECV_NOTES>
