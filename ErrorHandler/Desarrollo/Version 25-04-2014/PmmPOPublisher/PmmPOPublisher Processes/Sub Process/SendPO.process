                <commit>false</commit>
                <maxRows>%%Connections/Database/PMM/GLB_MAX_ROWS%%</maxRows>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>select a.prim_org_lvl_number, a.pmg_po_number, v.vendor_number, to_char(a.PMG_RELEASE_DATE, 'yyyy-mm-dd hh24:mi:ss') as PMG_RELEASE_DATE, to_char(a.PMG_ENTRY_DATE,'yyyy-mm-dd hh24:mi:ss') as PMG_ENTRY_DATE, 
a.CURR_CODE, p.PMG_SHORT_CODE, a.pmg_user, vp.VPC_PO_PROC_TIME, vp.VPC_PO_SHIP_TIME,
pn.PMG_PO_NOTES,  pn.PMG_RECV_NOTES, to_char(a.PMG_CNCL_BY_DATE, 'yyyy-mm-dd hh24:mi:ss') as PMG_CNCL_BY_DATE, 
to_char(a.PMG_EXP_RCT_DATE, 'yyyy-mm-dd hh24:mi:ss') as PMG_EXP_RCT_DATE,
va.VPC_EXT_PAY_DAYS, a.DMT_CODE, a.USR_TECH_KEY, to_char(a.pmg_last_chg_dt, 'yyyy-mm-dd hh24:mi:ss') as pmg_last_chg_dt, v.VPC_PO_REQUIRED, NVL(a.PMG_TOT_PO_COST,0) + NVL(a.TOT_CHG_EFFECT,0) - (NVL(a.TOT_ALL_EFFECT,0)*-1) AS PMG_TOT_PO_COST, v.VENDOR_NAME
from pmm.pmghdree a
left join pmm.vpcmstee v on a.vpc_tech_key = v.vpc_tech_key
left join pmm.PMGOTPEE p on a.pmg_type_code = p.pmg_type_code
LEFT JOIN pmm.VPCPOPEE vp on a.vpc_tech_key = vp.vpc_tech_key
    and a.vpc_shp_point = vp.vpc_shp_point
LEFT JOIN pmm.PMGNPOEE pn on a.pmg_po_number = pn.pmg_po_number
left join pmm.vpcpopee ve on a.vpc_tech_key = ve.vpc_tech_key
    and a.vpc_shp_point = ve.vpc_shp_point
left join pmm.vpcaptee va on ve.vpc_apt_key = va.vpc_apt_key
where a.pmg_po_number =  ? and LTRIM(RTRIM(VENDOR_NUMBER)) = ?</statement>
